          "patterns": [
            {
              "match": "\\b(0(x|X)[0-9A-Fa-f]*)\\b",
              "name": "constant.numeric.hex.parigp"
            },
            {
              "match": "\\b(0(b|B)[0-1]*)\\b",
              "name": "constant.numeric.bin.parigp"
            },
            {
              "match": "([0-9]*\\.[0-9]*\\s*([eE][+-]?[0-9]+)?)\\b",
              "name": "constant.numeric.float.parigp"
            },
